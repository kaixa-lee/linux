{
    "files.associations": {
        "*.rs": "rust",
        "*.vue": "perl",
        "*.bbclass": "python",
        "*.bb": "shellscript",
        "*.h": "c",
        "typeinfo": "cpp",
        "err.h": "c",
        "slab.h": "c",
        "random": "c",
        "*.tcc": "c",
        "chrono": "c",
        "streambuf": "c",
        "type_traits": "c",
        "array": "c",
        "string": "c",
        "string_view": "c",
        "compare": "c",
        "functional": "c",
        "tuple": "c",
        "utility": "c",
        "typeindex": "c",
        "list": "c",
        "spinlock.h": "c",
        "list.h": "c",
        "refcount.h": "c",
        "atomic.h": "c",
        "initializer_list": "c",
        "types.h": "c",
        "stddef.h": "c",
        "memory": "c",
        "numeric": "c",
        "cstdarg": "c",
        "ostream": "c",
        "limits": "c",
        "algorithm": "c",
        "unordered_map": "c",
        "unordered_set": "c",
        "memory_resource": "c",
        "deque": "c",
        "forward_list": "c",
        "vector": "c",
        "system_error": "c"
    },
    "git.ignoreLimitWarning": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "arch/arm64/boot/dts/freescale/.*": true,

        "scripts": true,
        "Documentation": true,

        "arch/a[a-q,s-z]*": true,
        "arch/ar[a-l,n-z]": true,
        "arch/[b-z]*/": true,
        "arch/arm": true,
        "!arch/arm64/boot/dts/**/i*.dts": false,
        "!arch/arm64/boot/dts/**/i*.dtsi": false,
        "arch/arm64/boot/dts/[a-e]*": true,
        "arch/arm64/boot/dts/[g-z]*": true,
        "arch/arm64/boot/dts/freescale/[a-h]*": true,
        "arch/arm64/boot/dts/freescale/[j-n]*": true,
        "arch/arm64/boot/dts/freescale/[p-z]*": true,
        "arch/arm64/boot/dts/freescale/imx8-*": true,
        "arch/arm64/boot/dts/freescale/imx8[a-l]*": true,
        "arch/arm64/boot/dts/freescale/imx8[n-z]*": true,
        "arch/arm64/boot/dts/freescale/imx8m[a-o]*": true,
        "arch/arm64/boot/dts/freescale/imx8m[q-z]*": true,
        "arch/arm64/boot/dts/freescale/imx8mp[a-z]*": true,
        "arch/arm64/boot/dts/freescale/imx8mp-*": true,

        "drivers/staging": true,
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "arch/arm64/boot/dts/freescale/.*": true,

        "scripts": true,
        "Documentation": true,

        "arch/a[a-q,s-z]*": true,
        "arch/ar[a-l,n-z]": true,
        "arch/[b-z]*/": true,
        "arch/arm": true,
        "!arch/arm64/boot/dts/**/i*.dts": false,
        "!arch/arm64/boot/dts/**/i*.dtsi": false,
        "arch/arm64/boot/dts/[a-e]*": true,
        "arch/arm64/boot/dts/[g-z]*": true,
        "arch/arm64/boot/dts/freescale/[a-h]*": true,
        "arch/arm64/boot/dts/freescale/[j-n]*": true,
        "arch/arm64/boot/dts/freescale/[p-z]*": true,
        "arch/arm64/boot/dts/freescale/imx8-*": true,
        "arch/arm64/boot/dts/freescale/imx8[a-l]*": true,
        "arch/arm64/boot/dts/freescale/imx8[n-z]*": true,
        "arch/arm64/boot/dts/freescale/imx8m[a-o]*": true,
        "arch/arm64/boot/dts/freescale/imx8m[q-z]*": true,
        "arch/arm64/boot/dts/freescale/imx8mp[a-z]*": true,
        "arch/arm64/boot/dts/freescale/imx8mp-*": true,

        "drivers/staging": true,
    }
}
