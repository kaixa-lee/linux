{
    "files.associations": {
        "*.rs": "rust",
        "*.vue": "perl",
        "*.bbclass": "python",
        "*.bb": "shellscript",
        "device.h": "c",
        "of.h": "c",
        "nvmem-provider.h": "c",
        "mtd-abi.h": "c",
        "chrono": "c",
        "random": "c",
        "limits": "c",
        "algorithm": "c",
        "byteorder.h": "c",
        "refcount.h": "c",
        "*.tcc": "c"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
